<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomFont">
        <attr name="assetFontPath" format="string"/>
    </declare-styleable>

    <declare-styleable name="StrokedTextAttrs">
        <attr name="textStrokeColor" format="color"/>
        <attr name="textStrokeWidth" format="float"/>
    </declare-styleable>

    <declare-styleable name="CheckableDrawableIds">
        <attr name="checkedDrawableId" format="integer"/>
        <attr name="uncheckedDrawableId" format="integer"/>
        <attr name="defaultCheckedState" format="boolean"/>
    </declare-styleable>
</resources>